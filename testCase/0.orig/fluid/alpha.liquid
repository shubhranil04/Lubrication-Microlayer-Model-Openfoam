FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      alpha.liquid;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 0 0 0 0];

#include "../../system/runParams"

internalField   uniform 1;

boundaryField
{
	top
	{
		type	zeroGradient;
	}
	
	right
	{
		type	zeroGradient;
	}
	
	axis
	{
		type	empty;
	}
	
	back
	{
		type	wedge;
	}
	
	front
	{
		type	wedge;
	}
	
	fluid_to_solid
	{
		type	constantAlphaContactAngle;
		theta0	$CA;
		limit	gradient;
		value	uniform 1;
	}
}		

